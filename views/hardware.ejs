<!doctype html>
<html>
  <head>
    <title>Hardware Checkout</title>
    <link rel="stylesheet" type="text/css" href="/css/common.css" />
  </head>
  <body>
    <%- include header.ejs %>
		<% for(var i=0; i<data.length; i++) { %>
			<div class='hardware-card'>
				<p>
					<%= data[i].name %>
				</p>
				<p>
					<%= data[i].serial_num %>
				</p>
			</div>
    <% } %>
  </body>
</html>
