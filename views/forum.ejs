<!doctype html>
<html>
  <head>
    <title>Forum</title>
    <link rel="stylesheet" type="text/css" href="/css/common.css" />
  </head>
  <body>
    <%- include header.ejs %>
		<% for(var i=0; i<data.length; i++) { %>
			<div class='forum-card'>
				<p>
					<%= data[i].user_id %>
				</p>
				<p>
					<%= data[i].title %>
				</p>
				<p>
					<%= data[i].forum_body %>
				</p>
			</div>
    <% } %>
  </body>
</html>
